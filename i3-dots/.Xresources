URxvt*background: #0f111a
urxvt*foreground:#ffffff
URxvt*geometry: 80x30
URxvt*scrollBar: false


!-- Xft settings -- !
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintfull

! -- Fonts -- !
  URxvt.font:xft:Monospace-Bold:pixelsize=12.5
  URxvt.boldfont:xft:Monospace-Bold:pixelsize=12.5

! window padding
st.borderpx:          15
st.font:            JetBrainsMono-Medium:pixelsize= 15
URxvt.letterSpace: -1

URxvt.internalBorder:  10

urxvt.foreground:       #aaaaaa


st.opacity:        255


*color12: #0D72CA
*color4: #0D72CA

URxvt*inheritPixmap: true
URxvt*transparent: true
! -- URxvt*shading: 0 to 99 darkens, 101 to 200 lightens -- !
URxvt*shading:0

! -- URxvt.perl-ext-common: ...,tabbed,...  -- !

URxvt.tabbed.tabbar-fg: 4
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 3
URxvt.tabbed.tab-bg: 2

! -- xterm config -- !


xterm*faceName: Monospace


xterm*faceSize: 10
xterm*renderFont: true
xterm*BorderWidth: 15
XTerm*selectToClipboard: true


! special
*.foreground:   #c6c8d1
*.background:   #0f111a
*.cursorColor:  #d2d4de

! black
*.color0:       #0f111a
*.color8:       #6b7089

! red
*.color1:       #e27878
*.color9:       #e98989

! green
*.color2:       #b4be82
*.color10:      #c0ca8e

! yellow
*.color3:       #e2a478
*.color11:      #e9b189

! blue
*.color4:        #88C0D0
*.color12:       #88C0D0

! magenta
*.color5:       #a093c7
*.color13:      #ada0d3

! cyan
*.color6:        #88C0D0
*.color14:       #88C0D0

! white
*.color7:       #c6c8d1
*.color15:      #d2d4de

xterm*background:#10141c


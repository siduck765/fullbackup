URxvt*background: #10141c
urxvt*foreground:#ffffff
URxvt*geometry: 80x30
URxvt*scrollBar: false


!-- Xft settings -- !
Xft.dpi:        96
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintfull

! -- Fonts -- !
  URxvt.font:xft:Monospace-Bold:pixelsize=12.5
  URxvt.boldfont:xft:Monospace-Bold:pixelsize=12.5

! window padding
st.borderpx:          15
st.font:            FiraCode-Bold:pixelsize=14
URxvt.letterSpace: -1

URxvt.internalBorder:  10

urxvt.foreground:       #aaaaaa

*color12: #0D72CA
*color4: #0D72CA

URxvt*inheritPixmap: true
URxvt*transparent: true
! -- URxvt*shading: 0 to 99 darkens, 101 to 200 lightens -- !
URxvt*shading:0

! -- URxvt.perl-ext-common: ...,tabbed,...  -- !

URxvt.tabbed.tabbar-fg: 4
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 3
URxvt.tabbed.tab-bg: 2

! -- xterm config -- !


xterm*faceName: Monospace


xterm*faceSize: 10
xterm*renderFont: true
xterm*BorderWidth: 15
XTerm*selectToClipboard: true


! X colors.
! Generated by 'wal'
*foreground:        #f3e9e2
*background:        #111013
*.foreground:       #f3e9e2
*.background:       #111013
emacs*foreground:   #f3e9e2
emacs*background:   #111013
URxvt*foreground:   #f3e9e2
XTerm*foreground:   #f3e9e2
UXTerm*foreground:  #f3e9e2
URxvt*background:   [100]#111013
XTerm*background:   #111013
UXTerm*background:  #111013
URxvt*cursorColor:  #f3e9e2
XTerm*cursorColor:  #f3e9e2
UXTerm*cursorColor: #f3e9e2
URxvt*borderColor:  [100]#111013

